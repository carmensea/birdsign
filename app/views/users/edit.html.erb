<%= form_for @user do |user_form| %>
  <%= user_form.fields_for :setting do |f| %>
    Address1: <%= f.text_field :address1 %><br />
    Address2: <%= f.text_field :address2 %><br />
    City: <%= f.text_field :city %><br />
    State: <%= f.text_field :state %><br />
    Zip: <%= f.text_field :zip %><br />
    Email Notifications: <%= f.check_box :email_notifications %><br />
  <% end %>
  <%= user_form.submit %>
<% end %>
